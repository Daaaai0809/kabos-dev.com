{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "pnpm run check:format && pnpm run check:lint",
    "check:format": "biome format src/*",
    "check:lint": "biome lint src/*",
    "fix": "pnpm run fix:style && pnpm run fix:lint",
    "fix:style": "biome format --write src/*",
    "fix:lint": "biome lint --apply src/*"
  },
  "dependencies": {
    "next": "14.2.2",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "typescript": "^5"
  }
}
